#include<stdio.h>
void main()
{
    int i,j,k,n,p,f,found,index=0,pages[100],frames[10],fault=0;
printf("enter the number of pages");
scanf("%d",&n);
printf("ENter page reference sequence");
for(i=0;i<n;i++)
scanf("%d",&pages[i]);
printf("enter the number of frames");
scanf("%d",&f);

for(i=0;i<f;i++)
{
    frames[i]=-1;
}
for(i=0;i<n;i++)
{
    found=0;
    for(j=0;j<f;j++)
    {
        if(frames[j]==pages[i])
        {
            found=1;
            break;
        }
    }
    if(!found)
    {
        frames[index]=pages[i];
        index=(index+1)%f;
        fault++;
    }
    printf("\nFRAMES");
    for(k=0;k<f;k++)
    {
        printf("%d",frames[k]);
    }
}
printf("\nPagefault : %d",fault);
}
